{
  "title": "Azure Cloud Storage",
  "type": "object",
  "required": [
    "connectorName",
    "account_name",
    "access_key"
  ],
  "properties": {
    "connectorName": {
      "type": "string",
      "title": "Name of the connector",
      "default": "Unstract's Azure cloud storage"
    },
    "account_name": {
      "type": "string",
      "title": "Account Name",
      "description": "Account name of azure cloud storage",
      "default":""
    },
    "access_key": {
      "type": "string",
      "title": "Access Key",
      "format": "password",
      "description": "Access key of the corresponding account",
      "default":""
    }
  }
}
