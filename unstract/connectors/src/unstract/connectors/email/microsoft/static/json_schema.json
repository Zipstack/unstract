{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "access_token": {
            "type": "string",
            "title": "Access Token",
            "description": "OAuth2 access token"
        },
        "refresh_token": {
            "type": "string",
            "title": "Refresh Token",
            "description": "OAuth2 refresh token"
        }
    },
    "required": ["access_token", "refresh_token"]
}
