{
  "title": "Redis Queue",
  "description": "Redis queue used to connect with any types of Queue mechanism to push the data for manual review queue",
  "type": "object",
  "required": [
    "connectorName",
    "host",
    "port",
    "user"
  ],
  "properties": {
    "connectorName": {
      "type": "string",
      "title": "Name of the connector",
      "default": "Unstract's Redis Queue connector"
    },
    "host": {
      "type": "string",
      "title": "Host",
      "default": "localhost"
    },
    "port": {
      "type": "string",
      "title": "Port",
      "default": "6379"
    },
    "user": {
      "type": "string",
      "title": "User",
      "default": "default"
    },
    "password": {
      "type": "string",
      "title": "Password",
      "format": "password",
      "default": ""
    }
  }
}
